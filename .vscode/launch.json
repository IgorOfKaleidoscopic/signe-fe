{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "name": "Test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    },
    {
      "type": "node-terminal",
      "name": "Build only",
      "request": "launch",
      "command": "ng build --configuration=development --verbose=true --progress=true --optimization=false",
      "cwd": "${workspaceFolder}"
    }
  ]
}
